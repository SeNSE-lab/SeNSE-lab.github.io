@ARTICLE{Birdwell2007,
  author = {Birdwell, J. Alexander and Solomon, Joseph H. and Thajchayapong,
	Montakan and Taylor, Michael A. and Cheely, Matthew and Towal, R.
	Blythe and Conradt, Jorg and Hartmann, Mitra J. Z.},
  title = {{Biomechanical models for radial distance determination by the rat
	vibrissal system}},
  journal = {Journal of Neurophysiology},
  year = {{2007}},
  volume = {{98}},
  pages = {{2439-2455}},
  number = {{4}},
  month = {{OCT}},
  abstract = {{Rats use active, rhythmic movements of their whiskers to acquire
	tactile information about three-dimensional object features. There
	are no receptors along the length of the whisker; therefore all tactile
	information must be mechanically transduced back to receptors at
	the whisker base. This raises the question: how might the rat determine
	the radial contact position of an object along the whisker? We developed
	two complementary biomechanical models that show that the rat could
	determine radial object distance by monitoring the rate of change
	of moment ( or equivalently, the rate of change of curvature) at
	the whisker base. The first model is used to explore the effects
	of taper and inherent whisker curvature on whisker deformation and
	used to predict the shapes of real rat whiskers during deflections
	at different radial distances. Predicted shapes closely matched experimental
	measurements. The second model describes the relationship between
	radial object distance and the rate of change of moment at the base
	of a tapered, inherently curved whisker. Together, these models can
	account for recent recordings showing that some trigeminal ganglion
	(Vg) neurons encode closer radial distances with increased firing
	rates. The models also suggest that four and only four physical variables
	at the whisker base - angular position, angular velocity, moment,
	and rate of change of moment - are needed to describe the dynamic
	state of a whisker. We interpret these results in the context of
	our evolving hypothesis that neural responses in Vg can be represented
	using a state-encoding scheme that includes combinations of these
	four variables.}},
  affiliation = {{Hartmann, MJZ (Reprint Author), Northwestern Univ, Dept Mech, Dept
	Biomed Engn, 2145 Sheridan Rd, Evanston, IL 60208 USA. Northwestern
	Univ, Dept Mech, Dept Biomed Engn, Evanston, IL 60208 USA. Univ Maryland,
	Neurosci Program \& Cognit Sci, College Pk, MD 20742 USA. Univ Zurich,
	ETH, Inst Neuroinformat, Zurich, Switzerland.}},
  author-email = {{m-hartmann@northwestern.edu}},
  comment = {birdwell_jn_2007.pdf},
  doc-delivery-number = {{219KE}},
  doi = {10.1152/jn.00707.2006},
  file = {:/home/mark/working/senseweb/pubs/pdf/birdwell_jn_2007.pdf:PDF},
  issn = {{0022-3077}},
  journal-iso = {{J. Neurophysiol.}},
  keywords-plus = {{TRIGEMINAL GANGLION NEURONS; FOLLICLE-SINUS COMPLEX; MYSTACIAL VIBRISSAE;
	ACTIVE TOUCH; DISCRIMINATION; INNERVATION; RESPONSES; MOVEMENTS;
	RESONANCE; DYNAMICS}},
  language = {{English}},
