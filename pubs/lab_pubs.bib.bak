  doc-delivery-number = {{635GY}},
  doi = {10.1177/0278364908104468},
  file = {:/home/mark/working/senseweb/pubs/pdf/solomon_ijrr_2010.pdf:PDF},
  funding-acknowledgement = {{NSF {[}IOB-0446391, IIS-0613568]}},
  funding-text = {{This work was supported by NSF awards IOB-0446391 and IIS-0613568
	to MJZH.}},
  issn = {{0278-3649}},
  journal-iso = {{Int. J. Robot. Res.}},
  keywords = {{whisker array; tactile perception; feature extraction; cantilever
	beam; biomimetic whisker; whisker slip; rat whisker system; vibrissal
