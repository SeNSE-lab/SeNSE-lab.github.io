  keywords = {{cerebellar granule cells; mossy fibers; somatosensory; ingestive;
	grooming; exploration; whiskers; vibrissae; passive touch; active
	touch; active sensing}},
  keywords-plus = {{INFERIOR OLIVE; PURKINJE-CELLS; MOTOR CONTROL; SOMATOSENSORY CORTEX;
	DISCHARGE PATTERNS; MOSSY FIBER; PROJECTIONS; NEURONS; NUCLEUS; ORGANIZATION}},
