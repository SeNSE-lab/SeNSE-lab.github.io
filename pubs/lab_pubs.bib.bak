@INPROCEEDINGS{Schultz2005,
  author = {Schultz, AE and Solomon, JH and Peshkin, MA and Hartmann, MJ},
  title = {{Multifunctional whisker arrays for distance detection, terrain mapping,
	and object feature extraction}},
  booktitle = {{2005 IEEE International Conference on Robotics and Automation (ICRA),
	Vols 1-4}},
  year = {{2005}},
  series = {{IEEE INTERNATIONAL CONFERENCE ON ROBOTICS AND AUTOMATION}},
  pages = {{2588-2593}},
  address = {Barcelona, Spain},
  organization = {{IEEE}},
  publisher = {{IEEE}},
  note = {{IEEE International Conference on Robotics and Automation (ICRA),
	Barcelona, SPAIN, APR 18-22, 2005}},
  abstract = {{Several species of animals use whiskers to accurately navigate and
	explore objects in the absence of vision. We have developed inexpensive
	arrays of artificial whiskers based on strain-gage and Flex Sensor
	technologies that can be used either in passive ({''}dragging{''})
	mode, or in active ({''}whisking{''}) mode. In the present work we
	explore the range of functions that whisker arrays can serve on a
	rover. We demonstrate that when mounted on a rover, whisker arrays
	can (1) Detect obstacles and determine obstacle distance (2) Map
	terrain features (3) Determine ground and surface texture (4) Provide
	an estimate of rover speed (5) Identify ``slip{''} of the rover wheels,
	and (6) Perform 3-dimensional extraction of object shape. We discuss
	the potential use of whisker arrays on planetary rovers and as an
	investigative tool for exploring the encoding of sensory information
